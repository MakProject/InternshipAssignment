[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/demo/InternshipAssignment/backend
ExecStart=/home/ubuntu/demo/InternshipAssignment/backend/venv/bin/gunicorn --bind localhost:8000 backend.wsgi:application

[Install]
WantedBy=multi-user.target

